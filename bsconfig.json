{
  "name": "@graft/cultivar",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "examples",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": [
    "bs-log",
    "bs-bastet",
    "bs-express",
    "wonka",
    "@anuragsoni/routes"
  ],
  "ppx-flags": ["bs-log/ppx"],
  "bsc-flags": ["-bs-super-errors"],
  "warnings": {
    "error": "+5"
  },
  "namespace": true,
  "refmt": 3,
  "gentypeconfig": {
    "language": "typescript",
    "module": "es6",
    "shims": {
      "Express": "ExpressShim"
    }
  }
}

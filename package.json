{
  "name": "cultivar",
  "version": "1.0.0-alpha.12",
  "description": "Composable event handling with Wonka for HTTP and beyond",
  "repository": {
    "type": "git",
    "url": "git@github.com:bkonkle/cultivar.git"
  },
  "author": "Brandon Konkle <brandon@konkle.us> (http://konkle.us)",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc --incremental -p tsconfig.json",
    "build.watch": "tsc --watch --incremental -p tsconfig.build.json",
    "check.types": "tsc --pretty --noEmit",
    "format": "prettier --write",
    "format.all": "yarn format \"{src,packages}/**/*.{js,jsx,ts,tsx}\"",
    "lint": "yarn eslint 'src/**/*{.js,.ts}'",
    "lint.fix": "yarn lint --fix",
    "prepublish": "yarn build",
    "test": "NODE_ENV=test jest",
    "test.cov": "NODE_ENV=test jest --coverage",
    "test.watch": "yarn test --watch",
    "examples.basic": "ts-node examples/basic/Server.ts"
  },
  "dependencies": {
    "@apollographql/graphql-playground-html": "^1.6.26",
    "apollo-server-core": "^2.16.1",
    "apollo-server-express": "^2.17.0",
    "camelcase": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "graphql": "^15.3.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@graphql-codegen/add": "^2.0.1",
    "@graphql-codegen/cli": "^1.17.8",
    "@graphql-codegen/typescript": "^1.17.9",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-urql": "^2.0.1",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.10",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.6.0",
    "@types/recursive-readdir": "^2.2.0",
    "@types/supertest": "^2.0.10",
    "@types/type-is": "^1.6.3",
    "@types/yup": "^0.29.6",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "chalk": "^4.1.0",
    "dotenv-cli": "^3.2.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^5.0.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.13",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "nope-validator": "^0.11.3",
    "prettier": "^2.1.0",
    "recursive-readdir": "^2.2.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typeorm": "^0.2.25",
    "typescript": "4.0.2",
    "yup": "^0.29.3"
  },

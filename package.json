{
  "name": "cultivar",
  "version": "1.0.0",
  "description": "Composable event handling with Wonka for HTTP and beyond",
  "repository": {
    "type": "git",
    "url": "git@github.com:bkonkle/cultivar.git"
  },
  "author": "Brandon Konkle <brandon@konkle.us> (http://konkle.us)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc --incremental -p tsconfig.build.json",
    "build.watch": "tsc --watch --incremental -p tsconfig.build.json",
    "check.types": "tsc --pretty --noEmit",
    "db.backup": "ts-node --transpile-only src/scripts/db/backup.ts",
    "db.create": "ts-node --transpile-only src/scripts/db/create.ts",
    "db.drop": "ts-node --transpile-only src/scripts/db/drop.ts",
    "db.functions": "ts-node --transpile-only src/scripts/db/functions.ts",
    "db.reset": "yarn db.drop && yarn db.create && yarn db.migrate",
    "db.restore": "ts-node --transpile-only src/scripts/db/restore.ts",
    "db.tunnel": "ts-node --transpile-only src/scripts/db/tunnel.ts",
    "format": "prettier --write",
    "format.all": "yarn format \"{src,packages}/**/*.{js,jsx,ts,tsx}\"",
    "lint": "yarn eslint 'src/**/*{.js,.ts}'",
    "lint.fix": "yarn lint --fix",
    "test": "NODE_ENV=test jest",
    "test.cov": "NODE_ENV=test jest --coverage",
    "test.int": "NODE_ENV=test jest -i -c ./jest.integration.config.js --forceExit",
    "test.watch": "yarn test --watch"
  },
  "dependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "prettier": "^2.0.5",
    "status-code-enum": "^1.0.0",
    "wonka": "^4.0.14"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^1.17.6",
    "@graphql-codegen/cli": "^1.17.6",
    "@graphql-codegen/typescript": "^1.17.6",
    "@graphql-codegen/typescript-operations": "^1.17.6",
    "@graphql-codegen/typescript-urql": "^1.17.6",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.9",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.27",
    "@types/recursive-readdir": "^2.2.0",
    "babel-jest": "^26.2.2",
    "dotenv-cli": "^3.2.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "recursive-readdir": "^2.2.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "3.9.7"
  }
}
